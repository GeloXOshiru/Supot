-- Create a ScreenGui to hold the ImageLabel
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "ImageGUI"
screenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

-- Create an ImageLabel to display the image
local imageLabel = Instance.new("ImageLabel")
imageLabel.Name = "MyImage"
imageLabel.Size = UDim2.new(0, 500, 0, 500)  -- Adjust size as needed
imageLabel.Position = UDim2.new(0.25, 0, 0.25, 0)  -- Adjust position as needed
imageLabel.BackgroundTransparency = 1  -- Make the background transparent
imageLabel.Image = "rbxassetid://16004656079"
imageLabel.Parent = screenGui
